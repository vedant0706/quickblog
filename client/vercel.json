{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://gem-ai-server.vercel.app/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        }
      ]
    }
  ]
}